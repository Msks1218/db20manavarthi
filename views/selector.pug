extends layout

block content
  h1= title
  - let image_names = ['pic1.jpg','pic2.jpg','pic3.jpg','pic4.jpg','pic5.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]
      img(src=`images/${iname}` alt='Vegetables' style='width:300px; height:300px;')
  - }
  
  
